<!---Navigation Bar-->  
<nav>
  <div class="logo">
  </div>
  <ul>
    <li>The Fauclty of Computer Science</li>
  </ul>
</nav>

<!--Banner-->
<section class="banner">
  <div class="container">   
    <input id="searchBar" class="searchbar" type="text" placeholder="Search a course">
  <a id="btnSearch" class="btn-search"><i class="fa fa-search"></i></a>
    </div> 
    
  </div>
  <img class="banner-image" src="https://preview.ibb.co/bMi5Y6/banner_img.png" alt="monitoring" >
</section>

<div class="container">
  <div class="row">
  <% for (var i=0; i<degreeContent.length; i++){ %>
  
    <div class="mb-30 col-md-6 col-lg-4">
      <div class="card">
      <img class="card-icon" src="https://image.ibb.co/cFV8mR/monitoring.png" alt="monitoring" >
      <h3 class="card-title"><%= degreeContent[i].degree %></h3>
      <p class="card-text"><%= degreeContent[i].content %></p>
      
      <% let degreeName = degreeContent[i].degree.split(" ").join("-"); %>
      <a class="card-link" href="/degree-structure/<%= degreeName %>">Learn more</a>
      </div>
    </div> 
  
  <% } %>
  </div>
</div>
